<h1> This is the index page </h1>

<% @posts.each do |post| %>

<h1> <%= post.title %> </h1>
<h5> <%= post.rating %> </h5>
<h5> <%= post.location %> </h5>
<h5> <%= post.price %> </h5>
<h5> <%= post.review %> </h5>

<h6> Posted: <%= time_ago_in_words(post.created_at) %> </h6>

<% end %>

<%= link_to "New Post", new_post_path %>